    "babel-register": "^6.9.0",
    "body-parser": "^1.15.2",
    "debug": "^2.6.0",
    "dotenv": "^2.0.0",
    "ejs": "^2.5.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
