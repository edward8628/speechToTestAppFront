{
  "name": "frontendtemplate",
  "version": "1.0.0",
  "description": "frontend template for a fast start",
  "main": "server.js",
  "scripts": {
    "webpack": "webpack",
    "build:js": "webpack --config webpack-build.config.js",
    "build": "npm run clean && npm run build:js",
    "clean": "rm -rf build",
    "predeploy": "npm run build && NODE_ENV='production' npm start",
    "start": "./node_modules/babel-cli/bin/babel-node.js src/server.js"
  },
  "engines": {
    "node": "6.2.1",
    "npm": "3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edward8628/frontendtemplate.git"
  },
  "keywords": [
    "es6",
    "react",
    "redux",
    "react-router",
    "webpack",
    "babel"
  ],
  "author": "Feng Liu",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.9.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
